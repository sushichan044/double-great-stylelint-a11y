{
  "name": "@double-great/stylelint-a11y",
  "version": "2.0.0",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-great/stylelint-a11y.git"
  },
  "author": "@double-great",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y",
    "accessibility"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "pretty-quick": "pretty-quick",
    "prepublish": "npm run build",
    "pretest": "npm run lint -- --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "stylelint": ">=13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^15.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-preset-stylelint": "^5.0.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1"
  },
  "bugs": {
    "url": "https://github.com/double-great/stylelint-a11y/issues"
  },
  "homepage": "https://github.com/double-great/stylelint-a11y#readme",
  "dependencies": {
    "postcss": "^8.4.14"
  }
}
